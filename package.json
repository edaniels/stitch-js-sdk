{
  "name": "baas-admin-sdk",
  "private": true,
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/10gen/baas-admin-sdk.git"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn build-main && yarn build-module",
    "build-main": "tsc -p tsconfig.cjs.json",
    "build-module": "tsc -p tsconfig.esm.json",
    "clean": "rm -rf dist",
    "test": "jest",
    "watch": "tsc -w -p tsconfig.esm.json",
    "lint": "tslint --project tsconfig.esm.json 'src/**/*.ts' 'test/**/*.ts'",
    "lint-fix": "tslint --project tsconfig.esm.json --fix 'src/**/*.ts' 'test/**/*.ts'",
    "lint-check": "tslint-config-prettier-check ./tslint.json",
    "prettier": "prettier --write \"{,!(node_modules),!(dist)/**/}*.ts\""
  },
  "dependencies": {
    "@types/bson": "^4.0.4",
    "@types/jwt-decode": "^2.2.1",
    "bson": "4.0.4",
    "json2typescript": "^1.4.1",
    "jwt-decode": "^2.2.0",
    "query-string": "^5.1.1"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "cross-fetch": "^3.0.4",
    "fetch-mock": "^5.10.1",
    "form-data": "^3.0.0",
    "jest": "^25.4.0",
    "md5": "^2.2.1",
    "mock-browser": "^0.92.14",
    "mongodb": "^2.2.26",
    "mongodb-stitch": "3",
    "prettier": "^1.13.5",
    "rollup": "^0.60.7",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.2.1",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-typescript2": "^0.15.0",
    "rollup-plugin-uglify": "^4.0.0",
    "sinon": "^2.2.0",
    "ts-jest": "^25.4.0",
    "ts-sinon": "^1.2.0",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.8.3"
  }
}
